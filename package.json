{
	"name": "hubstaff",
	"displayName": "Hubstaff for VSCode ",
	"description": "See how much you worked today (using Hubstaff Time Tracking Service) in you favorite editor",
	"version": "0.0.1",
	"publisher": "mehrad",
	"repository": {
		"type": "git",
		"url": "https://github.com/mehrad77/hubstaff-vscode.git"
	},
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.hubstaff",
				"title": "Set Hubstaff API Key"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"axios": "^0.19.0"
	}
}
